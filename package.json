{
  "name": "grunt-muxml",
  "version": "0.1.5",
  "description": "An XML parsing and formatting plugin for Grunt",
  "license": "MIT",
  "repository": "t1st3/grunt-muxml",
  "author": {
    "name": "t1st3",
    "email": "contact@t1st3.com",
    "url": "t1st3.com"
  },
  "main": "tasks/muxml.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo tasks/*.js test.js && grunt && mocha",
    "coverage": "grunt && nyc --reporter=lcov --reporter=text-lcov npm test",
    "precodecov": "npm run coverage",
    "codecov": "cat coverage/lcov.info | ./node_modules/.bin/codecov"
  },
  "files": [
    "tasks"
  ],
  "keywords": [
    "gruntplugin",
    "xml",
    "parse",
    "format",
    "minify",
    "prettify",
    "strip",
    "filter"
  ],
  "dependencies": {
    "concat-stream": "^1.5.2",
    "muxml": "^0.2.3"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "grunt": "^1.0.1",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^11.0.0",
    "xo": "^0.19.0"
  },
  "clinton": {
    "rules": {
      "ava": false
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
